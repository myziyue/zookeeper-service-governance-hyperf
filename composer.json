{
    "name": "hyperf/hyperf",
    "description": "A coroutine framework that focuses on hyperspeed and flexibility. Building microservice or middleware with ease.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "microservice",
        "coroutine"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=7.2",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-swoole": ">=4.4",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^0.7",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "bandwidth-throttle/token-bucket": "^2.0",
        "doctrine/annotations": "^1.6",
        "doctrine/common": "@stable",
        "doctrine/inflector": "^1.3",
        "doctrine/instantiator": "^1.0",
        "domnikl/statsd": "^3.0.1",
        "egulias/email-validator": "^2.1",
        "elasticsearch/elasticsearch": "^7.0",
        "endclothing/prometheus_client_php": "^0.9.1",
        "fig/http-message-util": "^1.1.2",
        "friendsofphp/php-cs-fixer": "^2.14",
        "google/protobuf": "^3.6.1",
        "grpc/grpc": "^1.15",
        "guzzlehttp/guzzle": "^6.3",
        "influxdb/influxdb-php": "^1.15.0",
        "ircmaxell/random-lib": "^1.2",
        "jcchavezs/zipkin-opentracing": "^0.1.4",
        "jean85/pretty-package-versions": "^1.2",
        "jonahgeorge/jaeger-client-php": "^0.4.4",
        "laminas/laminas-mime": "^2.7",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-memory": "^1.0",
        "league/plates": "^3.3",
        "malukenho/docheader": "^0.1.6",
        "markrogoyski/math-php": "^0.49.0",
        "mockery/mockery": "^1.0",
        "monolog/monolog": "^1.24",
        "nesbot/carbon": "^2.0",
        "nikic/fast-route": "^1.3",
        "nikic/php-parser": "^4.1",
        "overtrue/flysystem-qiniu": "^1.0",
        "php-amqplib/php-amqplib": "^2.7",
        "php-di/php-di": "^6.0",
        "php-di/phpdoc-reader": "^2.0.1",
        "phpstan/phpstan": "^0.11.15",
        "phpunit/phpunit": "^7.0.0",
        "roave/better-reflection": "^4.0",
        "smarty/smarty": "^3.1",
        "squizlabs/php_codesniffer": "^3.4",
        "start-point/etcd-php": "^1.1",
        "swoole/ide-helper": "dev-master",
        "sy-records/think-template": "^2.0",
        "symfony/console": "^4.2",
        "symfony/finder": "^4.1",
        "symfony/property-access": "^4.3",
        "symfony/serializer": "^4.3",
        "twig/twig": "^2.12",
        "vlucas/phpdotenv": "^3.1",
        "xxtime/flysystem-aliyun-oss": "^1.5"
    },
    "replace": {
        "hyperf/rpc-client": "self.version",
        "hyperf/service-governance": "self.version",
        "hyperf/zookeeper": "self.version"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "Hyperf\\RpcClient\\": "src/rpc-client/src/",
            "Hyperf\\ServiceGovernance\\": "src/service-governance/src/",
            "Hyperf\\Zookeeper\\": "src/zookeeper/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\ServiceGovernance\\": "src/service-governance/tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {},
        "hyperf": {
            "config": [
                "Hyperf\\RpcClient\\ConfigProvider",
                "Hyperf\\ServiceGovernance\\ConfigProvider",
                "Hyperf\\Zookeeper\\ConfigProvider"
            ]
        }
    },
    "bin": [],
    "scripts": {
        "test": "./bin/co-phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "license-check": "docheader check src/ test/",
        "cs-fix": "php-cs-fixer fix $1",
        "json-fix": "./bin/composer-json-fixer",
        "analyse": "phpstan analyse --memory-limit 300M -l 3 -c phpstan.neon"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
